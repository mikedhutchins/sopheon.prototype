@using Sopheon.Domain.Responses;
@using Sopheon.Domain.Entities;
@model List<Gate>

@if (Model.Count > 0)
{

	foreach (var item in Model)
	{
	<div class="tile gate-tile p5">
		<div class="w30 left">
			@item.Name
		</div>
		<div class="w30 left">
			@item.ScheduledCompletion.ToShortDateString()
		</div>
		<div class="clear"></div>
	</div>
	}
}
else
{
	<h2>No Gates</h2>
}